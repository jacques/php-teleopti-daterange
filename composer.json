{
    "name": "jacques/php-teleopti-dateranges",
    "description": "Parse date ranges from TeleOpti Schedules",
    "license": "MPL-2.0",
    "type": "library",
    "keywords": [
        "carbon",
        "teleopti"
    ],
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@siberia.co.za",
            "homepage": "http://www.siberia.co.za/"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pcre": "*",
        "composer/pcre": "^3.0",
        "nesbot/carbon": "^2.59"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.31",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.6",
        "rector/rector": "^0.17.2",
        "vimeo/psalm": "^5.13"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\TeleOpti\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jacques\\TeleOpti\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
